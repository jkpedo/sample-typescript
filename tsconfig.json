{
  "compilerOptions": {
    "baseUrl": "./",
    "module": "commonjs",
    "outDir": "./dist/",
    "strict": true,
    "target": "ES2019",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "removeComments": true,
    "declaration" : false,
    "alwaysStrict" : true,
  },
  "include": [
    "www.ts"
  ]
}
